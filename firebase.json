{
  "functions": {
    "source": ".",
    "ignore": [
      "firebase.json",
      "firebase-debug.log",
      "**/.*",
      "**/node_modules/**",
      "**/public/**",
      "**/*.(ts|tsx|scss)",
      "(tslint|tsconfig).json"
    ]
  },
  "hosting": {
    "public": "dist",
    "rewrites": [
      {
        "source": "**/**",
        "function": "nextjs-server"
      }
    ],
    "ignore": [
      "dist/_next/cache/**",
      "dist/_next/server/**",
      "dist/_next/cache/**"
    ]
  }
}
