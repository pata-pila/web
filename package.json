{
  "name": "with-relay-modern",
  "version": "3.0.3",
  "description": "Example of Next.js with Relay Modern SSR",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "relay": "relay-compiler --src ./ --artifactDirectory ./__types__ --exclude '**/dist/**' '**/node_modules/**' '**/test/**'  '**/__generated__/**' '**/schema/**' --schema ./schema/schema.graphql --language typescript",
    "schema": "node graphql-proxy.js & graphql get-schema dev",
    "serve": "cross-env NODE_ENV=production firebase serve",
    "deploy": "ncp public dist && firebase deploy --debug"
  },
  "engines": {
    "node": "12"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@zeit/next-sass": "^1.0.1",
    "classnames": "^2.2.6",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^1.8.0",
    "firebase-admin": "^11.9.0",
    "firebase-functions": "^3.11.0",
    "graphql": "^14.6.0",
    "isomorphic-unfetch": "^3.1.0",
    "moment": "^2.29.1",
    "next": "^10.0.1",
    "next-fonts": "^1.1.0",
    "node-sass": "^5.0.0",
    "prismic-reactjs": "^1.3.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-intersection-observer": "^8.31.0",
    "react-markdown": "^5.0.2",
    "react-relay": "^9.1.0",
    "react-responsive": "^8.1.1"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.11",
    "@types/node": "^13.11.1",
    "@types/react": "^16.9.56",
    "@types/react-relay": "^7.0.17",
    "babel-plugin-relay": "^9.1.0",
    "firebase-tools": "^12.4.3",
    "graphql-cli": "^4.1.0",
    "moment-locales-webpack-plugin": "^1.2.0",
    "ncp": "^2.0.0",
    "prettier": "^2.0.5",
    "relay-compiler": "^9.1.0",
    "relay-compiler-language-typescript": "^12.0.3",
    "stylelint": "^13.6.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-prettier": "^1.1.2",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^3.9.5"
  }
}
